{
  "[css]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[markdown]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "breadcrumbs.enabled": false,
  "cSpell.language": "en,pl",
  "cSpell.useGitignore": false,
  "chat.disableAIFeatures": true,
  "custom-format.formatters": [
    { "command": "format.sh stdin --filename $FILE", "language": "plaintext" },
    { "command": "format.sh stdin --filetype c", "language": "c" },
    { "command": "format.sh stdin --filetype cpp", "language": "cpp" },
    { "command": "format.sh stdin --filetype cuda", "language": "cuda-cpp" },
    { "command": "format.sh stdin --filetype json", "language": "json" },
    { "command": "format.sh stdin --filetype jsonc", "language": "jsonc" },
    { "command": "format.sh stdin --filetype lua", "language": "lua" },
    { "command": "format.sh stdin --filetype py", "language": "python" },
    { "command": "format.sh stdin --filetype sh", "language": "shellscript" },
    { "command": "format.sh stdin --filetype vim", "language": "viml" }
  ],
  "editor.acceptSuggestionOnEnter": "on",
  "editor.cursorBlinking": "solid",
  "editor.cursorSurroundingLines": 1024,
  "editor.defaultFormatter": "Vehmloewff.custom-format",
  "editor.fontFamily": "'JetBrains Mono', monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.inlayHints.enabled": "on",
  "editor.lineNumbers": "off",
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": "multiFile",
  "editor.overviewRulerBorder": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.showFoldingControls": "never",
  "editor.suggest.matchOnWordStartOnly": false,
  "editor.tabCompletion": "on",
  "editor.wordWrap": "on",
  "extensions.experimental.affinity": {
    "jimmyzjx.leaderkey": 1,
    "vscodevim.vim": 1
  },
  "extensions.ignoreRecommendations": true,
  "files.associations": { "**/i3/config": "i3" },
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.saveConflictResolution": "overwriteFileOnDisk",
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.enabled": true,
  "git.openRepositoryInParentFolders": "always",
  "interactiveWindow.executeWithShiftEnter": true,
  "jupyter.askForKernelRestart": false,
  "keyboard.dispatch": "keyCode",
  "leaderkey.overrides.user": {
    ", e N": {
      "command": "go-to-next-problem.next",
      "name": "next error in file"
    },
    ", e P": {
      "command": "go-to-next-problem.prev",
      "name": "previous error in file"
    },
    ", e n": {
      "command": "go-to-next-problem.nextInFiles",
      "name": "next error"
    },
    ", e p": {
      "command": "go-to-next-problem.prevInFiles",
      "name": "previous error"
    }
  },
  "notebook.experimental.remoteSave": true,
  "notebook.formatOnCellExecution": true,
  "notebook.formatOnSave.enabled": true,
  "path-autocomplete.triggerOutsideStrings": true,
  "remote.SSH.defaultExtensions": [
    "charliermarsh.ruff",
    "davidanson.vscode-markdownlint",
    "dcasella.i3",
    "detachhead.basedpyright",
    "esbenp.prettier-vscode",
    "ionutvmi.path-autocomplete",
    "jacobdufault.fuzzy-search",
    "jimmyzjx.go-to-next-problem",
    "jimmyzjx.leaderkey",
    "jimmyzjx.remote-commons",
    "jimmyzjx.ripgrep",
    "llvm-vs-code-extensions.vscode-clangd",
    "mechatroner.rainbow-csv",
    "ms-python.python",
    "ms-toolsai.jupyter",
    "ms-toolsai.jupyter-keymap",
    "ms-toolsai.jupyter-renderers",
    "ms-vscode-remote.remote-ssh",
    "ms-vscode-remote.remote-ssh-edit",
    "ms-vscode.hexeditor",
    "ms-vscode.makefile-tools",
    "stkb.rewrap",
    "streetsidesoftware.code-spell-checker",
    "streetsidesoftware.code-spell-checker-polish",
    "timonwong.shellcheck",
    "vehmloewff.custom-format",
    "vscodevim.vim",
    "xadillax.viml"
  ],
  "remote.SSH.lockfilesInTmp": true,
  "remote.extensionKind": {
    "jimmyzjx.leaderkey": ["ui"],
    "pub.name": ["ui"],
    "vscodevim.vim": ["ui"]
  },
  "security.workspace.trust.banner": "always",
  "security.workspace.trust.startupPrompt": "always",
  "shellcheck.customArgs": ["--exclude=SC1091,SC2312", "--enable=all"],
  "shellcheck.disableVersionCheck": true,
  "telemetry.enableTelemetry": false,
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cwd": "${relativeFileDirname}",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.localEchoEnabled": "off",
  "terminal.integrated.suggest.enabled": false,
  "terminal.integrated.tabs.enabled": false,
  "update.showReleaseNotes": false,
  "vim.handleKeys": { "<C-l>": false },
  "vim.hlsearch": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    { "before": ["<C-a>"], "commands": ["editor.action.selectAll"] }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    { "comment": "normal/visual mode keybindings" },
    {
      "before": ["<leader>"],
      "commands": [
        { "args": ["leaderkeyState", "SPC"], "command": "_setContext" },
        { "args": "SPC", "command": "leaderkey.render" }
      ]
    },
    {
      "before": [","],
      "commands": [
        { "args": ["leaderkeyState", ","], "command": "_setContext" },
        { "args": ",", "command": "leaderkey.render" }
      ]
    },
    { "after": ["g", "j"], "before": ["j"] },
    { "after": ["g", "k"], "before": ["k"] },
    { "after": ["%"], "before": ["%"] },
    { "before": ["<C-o>"], "commands": ["workbench.action.navigateBack"] },
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.addSelectionToNextFindMatch"]
    },
    { "before": ["<C-f>"], "commands": ["actions.find"] },
    { "before": ["<C-p>"], "commands": ["workbench.action.quickOpen"] },
    { "before": ["<C-s>"], "commands": ["workbench.action.files.save"] },
    { "before": ["<C-S-p>"], "commands": ["workbench.action.showCommands"] },
    { "before": ["<C-S-z>"], "commands": ["redo"] },
    { "before": ["<C-z>"], "commands": ["undo"] },
    { "comment": "normal mode keybindings" },
    { "before": ["u"], "commands": ["undo"] },
    { "before": ["<C-r>"], "commands": ["redo"] },
    { "before": ["K"], "commands": ["editor.action.showHover"] }
  ],
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    { "comment": "normal/visual mode keybindings" },
    {
      "before": ["<leader>"],
      "commands": [
        { "args": ["leaderkeyState", "SPC"], "command": "_setContext" },
        { "args": "SPC", "command": "leaderkey.render" }
      ]
    },
    {
      "before": [","],
      "commands": [
        { "args": ["leaderkeyState", ","], "command": "_setContext" },
        { "args": ",", "command": "leaderkey.render" }
      ]
    },
    { "after": ["g", "j"], "before": ["j"] },
    { "after": ["g", "k"], "before": ["k"] },
    { "after": ["%"], "before": ["%"] },
    { "before": ["<C-o>"], "commands": ["workbench.action.navigateBack"] },
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.addSelectionToNextFindMatch"]
    },
    { "before": ["<C-f>"], "commands": ["actions.find"] },
    { "before": ["<C-p>"], "commands": ["workbench.action.quickOpen"] },
    { "before": ["<C-s>"], "commands": ["workbench.action.files.save"] },
    { "before": ["<C-S-p>"], "commands": ["workbench.action.showCommands"] },
    { "before": ["<C-S-z>"], "commands": ["redo"] },
    { "before": ["<C-z>"], "commands": ["undo"] },
    { "comment": "visual mode keybindings" },
    { "after": ["p", "g", "v", "y"], "before": ["p"] },
    { "after": [">", "g", "v"], "before": [">"] },
    { "after": ["<", "g", "v"], "before": ["<"] }
  ],
  "window.autoDetectColorScheme": true,
  "window.commandCenter": false,
  "window.menuBarVisibility": "toggle",
  "window.openFoldersInNewWindow": "off",
  "window.titleBarStyle": "native",
  "workbench.activityBar.location": "hidden",
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.showTabs": "none",
  "workbench.enableExperiments": false,
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": false,
  "workbench.tips.enabled": false,
  "workbench.welcomePage.walkthroughs.openOnInstall": false
}
